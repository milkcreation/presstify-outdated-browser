{
  "name": "presstify-plugins/outdated-browser",
  "description": "Outdated Browser PresstiFy Plugin.",
  "homepage": "https://svn.tigreblanc.fr/presstify-plugins/outdated-browser/",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "outdated-browser",
    "web browser",
    "out of date",
    "deprecated"
  ],
  "type": "presstify-plugin",
  "time": "2021-05-03",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "presstify/framework": ">=1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "tiFy\\Plugins\\OutdatedBrowser\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tiFy\\Plugins\\OutdatedBrowser\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
